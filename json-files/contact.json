"contact": {
  "type": "contactForm",
  "title": "Contact",
  "content": {
    "wrapper": {
      "tag": "div",
      "attributes": {
        "class": "container"
      }
    },
    "form": {
      "tag": "form",
      "attributes": {
        "id": "contact",
        "action": "",
        "method": "post"
      },
      "headers": [
        { "tag": "h3", "text": "Quick Contact" },
        { "tag": "h4", "text": "Contact us today, and get reply with in 24 hours!" }
      ]
    },
    "fields": [
      {
        "tag": "input",
        "wrapperTag": "fieldset",
        "attributes": {
          "placeholder": "Your name",
          "type": "text",
          "tabindex": "1",
          "required": true,
          "autofocus": true
        }
      },
      {
        "tag": "input",
        "wrapperTag": "fieldset",
        "attributes": {
          "placeholder": "Your Email Address",
          "type": "email",
          "tabindex": "2",
          "required": true
        }
      },
      {
        "tag": "input",
        "wrapperTag": "fieldset",
        "attributes": {
          "placeholder": "Your Phone Number",
          "type": "tel",
          "tabindex": "3",
          "required": true
        }
      },
      {
        "tag": "input",
        "wrapperTag": "fieldset",
        "attributes": {
          "placeholder": "Your Web Site starts with http://",
          "type": "url",
          "tabindex": "4",
          "required": true
        }
      },
      {
        "tag": "textarea",
        "wrapperTag": "fieldset",
        "attributes": {
          "placeholder": "Type your Message Here....",
          "tabindex": "5",
          "required": true
        }
      },
      {
        "tag": "button",
        "text": "Submit",
        "wrapperTag": "fieldset",
        "attributes": {
          "name": "submit",
          "type": "submit",
          "id": "contact-submit",
          "data-submit": "...Sending"
        }
      }
    ]
  }
}
