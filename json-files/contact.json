{
  "title": "Contact",
  "contactForm": {
    "wrapper": {
      "tag": "section",
      "attributes": {
        "class": "body-paragraphs",
        "aria-labelledby": "contact-page-heading"
      }
    },
    "form": {
      "attributes": {
        "method": "post",
        "action": "",
        "class": "ccform"
      }
    },
    "fields": [
      {
        "type": "text",
        "icon": "fa-user",
        "placeholder": "Your Name",
        "required": true
      },
      {
        "type": "email",
        "icon": "fa-envelope",
        "placeholder": "Email",
        "required": true
      },
      {
        "type": "text",
        "icon": "fa-info",
        "placeholder": "Subject",
        "required": true
      },
      {
        "type": "textarea",
        "icon": "fa-comment",
        "placeholder": "Message",
        "name": "comments",
        "rows": 8,
        "required": true
      },
      {
        "type": "submit",
        "value": "Submit"
      }
    ]
  }
}
